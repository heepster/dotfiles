{
  "vscode-neovim.neovimPath": "/usr/bin/nvim",
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.lineHeight": 22,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "[DuoTone Dark Sky]": {
      "activityBar.background": "#221f3b",
      "statusBar.background": "#2f2b4f",
      "sideBar.background": "#34314d",
      "tab.inactiveBackground": "#34314d",
      "titleBar.activeBackground": "#34314d"
    },
    "[DuoTone Dark Sea]": {
      "activityBar.background": "#1E2D43",
      "statusBar.background": "#1E2D43",
      "sideBar.background": "#1E2D43",
      "tab.inactiveBackground": "#1E2D43",
      "titleBar.activeBackground": "#1E2D43"
    },
  },
  "terminal.integrated.lineHeight": 1.3,
  "window.zoomLevel": 2,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "files.trimTrailingWhitespace": true,
  "go.gopath": "/usr/local/Cellar/go/1.15/bin",
  "go.formatTool": "gofmt",
  "explorer.confirmDelete": false,
  "terminal.integrated.shell.linux": "/usr/bin/zsh",
  "window.enableMenuBarMnemonics": false,
  "terminal.integrated.scrollback": 100000,
  "go.toolsManagement.autoUpdate": true,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "terminal.integrated.macOptionIsMeta": true
}
